  "author": "Dominic Garms",
  "repository": "https://github.com/dohomi/tamagui-kitchen-sink",
  "homepage": "https://tamagui-extras.vercel.app",
  "version": "1.8.5",
  "keywords": [
    "react",
    "react-native",
    "@tamagui/core",
    "tamagui",
    "react-hook-form"
  ],
  "source": "src/index.ts",
  "types": "./types/index.d.ts",
  "main": "dist/cjs",
  "module": "dist/esm",
  "module:jsx": "dist/jsx",
  "files": [
    "src",
    "types",
    "dist"
  ],
  "scripts": {
    "build": "tamagui-build",
    "watch": "tamagui-build --watch",
    "clean": "tamagui-build clean",
    "clean:build": "tamagui-build clean:build",
    "check-types": "tsc --noEmit",
    "release-minor": "yarn build && npx release minor",
    "release-patch": "yarn build && npx release patch",
    "release-major": "yarn build && npx release major",
    "npmPublish": "npm publish"
  },
  "devDependencies": {
    "@tamagui-extras/config": "1.6.2",
    "@tamagui/build": "^1.79.11",
    "prettier": "^2.8.7",
