    "react-native-svg": "^13.9.0",
    "tamagui": "^1.79.11"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*",
    "react-native-svg": "^13.9.0",
    "tamagui": "1.29.3"
  },
  "publishConfig": {
    "access": "public"
  }
