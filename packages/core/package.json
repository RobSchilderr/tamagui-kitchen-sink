{
  "name": "@tamagui-extras/core",
  "license": "MIT",
  "author": "Dominic Garms",
  "repository": "https://github.com/dohomi/tamagui-kitchen-sink",
  "homepage": "https://tamagui-extras.vercel.app",
  "version": "0.17.0",
  "keywords": [
    "react",
    "react-native",
    "@tamagui/core",
    "tamagui",
    "react-hook-form"
  ],
  "source": "src/index.ts",
  "types": "./types/index.d.ts",
  "main": "dist/cjs",
  "module": "dist/esm",
  "module:jsx": "dist/jsx",
  "sideEffects": [
    "*.css"
  ],
  "files": [
    "src",
    "types",
    "dist"
  ],
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./types/index.d.ts",
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.js"
    }
  },
  "scripts": {
    "build": "tamagui-build",
    "watch": "tamagui-build --watch",
    "clean": "tamagui-build clean",
    "clean:build": "tamagui-build clean:build",
    "check-types": "tsc --noEmit",
    "release-minor": "yarn build && npx release minor",
    "release-patch": "yarn build && npx release patch",
    "release-major": "yarn build && npx release major",
    "npmPublish": "npm publish"
  },
  "devDependencies": {
    "@storybook/react": "^7.0.4",
    "@tamagui/build": "1.15.13",
    "@tamagui/themes": "1.15.13",
    "prettier": "^2.8.7",
    "react-native": "^0.71.6",
    "solito": "^3.0.1",
    "tamagui": "1.15.13",
    "tamagui-phosphor-icons": "0.2.0"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*",
    "solito": "3.0.1",
    "tamagui": "1.15.13",
    "tamagui-phosphor-icons": "0.2.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
