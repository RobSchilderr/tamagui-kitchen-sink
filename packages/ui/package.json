{
  "name": "tamagui-extras",
  "license": "MIT",
  "author": "Dominic Garms",
  "version": "0.12.3",
  "repository": "https://github.com/dohomi/tamagui-kitchen-sink",
  "homepage": "https://tamagui-extras.vercel.app",
  "sideEffects": [
    "*.css"
  ],
  "keywords": [
    "react",
    "react-native",
    "@tamagui/core",
    "tamagui",
    "react-hook-form"
  ],
  "types": "./types/index.d.ts",
  "main": "dist/cjs",
  "module": "dist/esm",
  "module:jsx": "src",
  "files": [
    "src/animations.ts",
    "src/global.ts",
    "src/tamagui.config.ts",
    "dist",
    "types/index.d.ts",
    "types/core",
    "tamagui.config.ts",
    "link",
    "youtube",
    "richText",
    "video",
    "form",
    "file"
  ],
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./types/index.d.ts",
      "require": "./dist/esm/index.js",
      "import": "./dist/esm/index.js"
    },
    "./*": {
      "types": "./*/index.d.ts",
      "require": "./dist/esm/*/index.js",
      "import": "./dist/esm/*/index.js"
    }
  },
  "scripts": {
    "build": "rm -rf dist types && tamagui-build src/index.tsx && yarn ts",
    "watch": "tamagui-build src/index.tsx --watch",
    "check-types": "tsc --noEmit",
    "ts": "tsc --emitDeclarationOnly --declarationDir ./ --outDir ./typings --rootDir ./src --tsBuildInfoFile ./tsconfig.tsbuildinfo --declarationMap",
    "ts:clean": "tsc --build --clean && rm -f $(find . -type f -name \"*.d.ts\" -o -name \"*.d.ts.map\") && rm -rf core file form link richText video youtube",
    "release-minor": "yarn build && npx release minor",
    "release-patch": "yarn build && npx release patch",
    "release-major": "yarn build && npx release major",
    "npmPublish": "npm publish"
  },
  "dependencies": {
    "@datepicker-react/hooks": "^2.8.4",
    "@siposdani87/expo-rich-text-editor": "^0.9.2",
    "@tiptap/pm": "2.0.0-beta.220",
    "@tiptap/react": "2.0.0-beta.220",
    "@tiptap/starter-kit": "2.0.0-beta.220",
    "expo-av": "^13.2.1",
    "expo-document-picker": "^11.2.2",
    "react-hook-form": "^7.43.9",
    "react-native-svg": "^13.9.0",
    "react-native-webview": "^11.26.1"
  },
  "devDependencies": {
    "@storybook/react": "^7.0.2",
    "@tamagui/build": "1.11.2",
    "@tamagui/themes": "1.11.2",
    "prettier": "^2.8.7",
    "solito": "^3.0.0",
    "tamagui": "1.11.2",
    "tamagui-phosphor-icons": "0.2.0",
    "typescript": "5.0.3"
  },
  "peerDependencies": {
    "solito": ">=3.0.0",
    "tamagui": "1.11.2",
    "tamagui-phosphor-icons": "0.2.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
