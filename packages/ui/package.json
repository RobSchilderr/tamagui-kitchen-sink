{
  "name": "tamagui-extras",
  "license": "MIT",
  "author": "Dominic Garms",
  "version": "0.0.50",
  "repository": "https://github.com/dohomi/tamagui-kitchen-sink",
  "homepage": "https://tamagui-extras.vercel.app",
  "sideEffects": [
    "*.css"
  ],
  "keywords": [
    "react",
    "react-native",
    "@tamagui/core",
    "tamagui",
    "react-hook-form"
  ],
  "types": "./types/index",
  "main": "dist/cjs",
  "module": "dist/esm",
  "module:jsx": "dist/jsx",
  "files": [
    "types",
    "dist"
  ],
  "scripts": {
    "build": "rm -rf dist types && tamagui-build src/index.tsx",
    "watch": "tamagui-build src/index.tsx --watch",
    "check-types": "tsc --noEmit",
    "release-minor": "yarn build && npx release minor",
    "release-patch": "yarn build && npx release patch",
    "release-major": "yarn build && npx release major"
  },
  "dependencies": {
    "@datepicker-react/hooks": "^2.8.4",
    "@mdi/js": "^7.1.96",
    "@siposdani87/expo-rich-text-editor": "^0.9.1",
    "@tamagui/config-base": "1.0.1-rc.6",
    "@tamagui/lucide-icons": "1.0.1-rc.6",
    "@tiptap/core": "^2.0.0-beta.209",
    "@tiptap/react": "^2.0.0-beta.209",
    "@tiptap/starter-kit": "^2.0.0-beta.209",
    "expo-av": "^13.0.2",
    "expo-document-picker": "^11.0.1",
    "prosemirror-commands": "^1.5.0",
    "prosemirror-dropcursor": "^1.6.1",
    "prosemirror-gapcursor": "^1.3.1",
    "prosemirror-history": "^1.3.0",
    "prosemirror-keymap": "^1.2.0",
    "prosemirror-schema-list": "^1.2.2",
    "react-hook-form": "^7.34.2",
    "react-native-svg": "^13.6.0",
    "react-native-webview": "^11.26.0",
    "rooks": "^7.4.2",
    "solito": "^2.1.2",
    "tamagui": "1.0.1-rc.6"
  },
  "devDependencies": {
    "@storybook/react": "^6.5.12",
    "@tamagui/build": "1.0.1-rc.6",
    "@types/quill": "^2.0.9"
  },
  "publishConfig": {
    "access": "public"
  }
}
