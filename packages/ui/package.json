{
  "name": "tamagui-extras",
  "license": "MIT",
  "author": "Dominic Garms",
  "version": "0.14.0",
  "repository": "https://github.com/dohomi/tamagui-kitchen-sink",
  "homepage": "https://tamagui-extras.vercel.app",
  "sideEffects": [
    "*.css"
  ],
  "keywords": [
    "react",
    "react-native",
    "@tamagui/core",
    "tamagui",
    "react-hook-form"
  ],
  "types": "./index.d.ts",
  "main": "dist/esm/index.js",
  "module": "dist/esm/index.mjs",
  "files": [
    "dist",
    "index.d.ts",
    "index.d.ts.map",
    "core",
    "date",
    "file",
    "form",
    "link",
    "richText",
    "video",
    "youtube"
  ],
  "scripts": {
    "rm:all": "rm -rf out dist types",
    "build:tsup": "yarn rm:all && tsup --config ./tsup.config.ts && yarn ts",
    "build": "yarn rm:all && tamagui-build --skip-types && yarn ts",
    "watch": "tamagui-build --watch",
    "check-types": "tsc --noEmit",
    "ts": "tsc --emitDeclarationOnly --declarationDir ./ --outDir ./typings --rootDir ./src --tsBuildInfoFile ./tsconfig.tsbuildinfo --declarationMap",
    "ts:clean": "tsc --build --clean && rm -f $(find . -type f -name \"*.d.ts\" -o -name \"*.d.ts.map\")",
    "release-minor": "yarn build && npx release minor",
    "release-patch": "yarn build && npx release patch",
    "release-major": "yarn build && npx release major",
    "npmPublish": "npm publish"
  },
  "dependencies": {
    "@datepicker-react/hooks": "^2.8.4",
    "@react-native-community/datetimepicker": "^7.0.1",
    "@siposdani87/expo-rich-text-editor": "^0.9.2",
    "@tiptap/pm": "2.0.0",
    "@tiptap/react": "2.0.0",
    "@tiptap/starter-kit": "2.0.0",
    "burnt": "^0.10.0",
    "expo-av": "^13.2.1",
    "expo-document-picker": "^11.2.2",
    "react-hook-form": "^7.43.9",
    "react-native-modal-datetime-picker": "^15.0.0",
    "react-native-svg": "^13.9.0",
    "react-native-webview": "^11.26.1"
  },
  "devDependencies": {
    "@storybook/react": "^7.0.4",
    "@tamagui/build": "1.14.4",
    "@tamagui/themes": "1.14.4",
    "prettier": "^2.8.7",
    "react-native": "^0.71.6",
    "solito": "^3.0.1",
    "tamagui": "1.14.4",
    "tamagui-phosphor-icons": "0.2.0",
    "tsup": "6.7.0",
    "typescript": "5.0.3"
  },
  "peerDependencies": {
    "solito": "3.0.1",
    "tamagui": "1.14.4",
    "tamagui-phosphor-icons": "0.2.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
