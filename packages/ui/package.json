{
  "name": "tamagui-extras",
  "license": "MIT",
  "author": "Dominic Garms",
  "version": "0.13.0",
  "repository": "https://github.com/dohomi/tamagui-kitchen-sink",
  "homepage": "https://tamagui-extras.vercel.app",
  "sideEffects": [
    "*.css"
  ],
  "keywords": [
    "react",
    "react-native",
    "@tamagui/core",
    "tamagui",
    "react-hook-form"
  ],
  "types": "./index.d.ts",
  "main": "dist/esm/index.js",
  "module": "dist/esm/index.js",
  "files": [
    "dist",
    "index.d.ts",
    "core",
    "date",
    "file",
    "form",
    "link",
    "richText",
    "video",
    "youtube"
  ],
  "exports": {
    "./package.json": "./package.json",
    ".": "./dist/esm/index.js",
    "./date": "./dist/esm/date/index.js",
    "./file": "./dist/esm/file/index.js",
    "./form": "./dist/esm/form/index.js",
    "./link": "./dist/esm/link/index.js",
    "./richText": "./dist/esm/richText/index.js",
    "./video": "./dist/esm/video/index.js",
    "./youtube": "./dist/esm/youtube/index.js"
  },
  "scripts": {
    "build": "rm -rf dist types && tamagui-build && yarn ts",
    "watch": "tamagui-build src/index.tsx --watch",
    "check-types": "tsc --noEmit",
    "build:2": "npx esbuild ./src/ --outdir=. --bundle --format=esm --platform=node",
    "ts": "tsc --emitDeclarationOnly --declarationDir ./ --outDir ./typings --rootDir ./src --tsBuildInfoFile ./tsconfig.tsbuildinfo --declarationMap",
    "ts:clean": "tsc --build --clean && rm -f $(find . -type f -name \"*.d.ts\" -o -name \"*.d.ts.map\")",
    "release-minor": "yarn build && npx release minor",
    "release-patch": "yarn build && npx release patch",
    "release-major": "yarn build && npx release major",
    "npmPublish": "npm publish"
  },
  "dependencies": {
    "@datepicker-react/hooks": "^2.8.4",
    "@siposdani87/expo-rich-text-editor": "^0.9.2",
    "@tiptap/pm": "2.0.0",
    "@tiptap/react": "2.0.0",
    "@tiptap/starter-kit": "2.0.0",
    "burnt": "^0.10.0",
    "expo-av": "^13.2.1",
    "expo-document-picker": "^11.2.2",
    "react-hook-form": "^7.43.9",
    "react-native-svg": "^13.9.0",
    "react-native-webview": "^11.26.1"
  },
  "devDependencies": {
    "@storybook/react": "^7.0.2",
    "@tamagui/build": "1.14.0",
    "@tamagui/themes": "1.14.0",
    "prettier": "^2.8.7",
    "solito": "^3.0.1",
    "tamagui": "1.14.0",
    "tamagui-phosphor-icons": "0.2.0",
    "typescript": "5.0.3"
  },
  "peerDependencies": {
    "solito": "3.0.1",
    "tamagui": "1.14.0",
    "tamagui-phosphor-icons": "0.2.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
